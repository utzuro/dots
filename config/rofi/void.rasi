* {
  background-color: rgba(24, 0, 37, 100%);
  text-color: rgba(255, 252, 255, 100%);
  width: 50%;
  height: 60%;
}

prompt {
  padding: 0.3em;
}

entry {
  padding: 0.3em;
	cursor: pointer;
	cursor-color: rgb(220,20,60);
	cursor-width: 0.3em;
}

element {
  padding: 0.3em;
	spacing: 1em;
}

element-icon {
		size: 2em;
}

element-text {
		vertical-align: 0.5;
}
