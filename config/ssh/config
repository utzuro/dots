Host gitlab.com
	HostName gitlab.com
	User git
	AddKeysToAgent yes
	IdentityFile ~/.ssh/utzuro

Host gitlab.com-custom
	HostName gitlab.com
	User git
	AddKeysToAgent yes
	IdentityFile ~/.ssh/custom

Host github.com
	HostName github.com
	User git
	AddKeysToAgent yes
	IdentityFile ~/.ssh/utzuro

Host ssh.custom
	HostName 124.0.0.0
    Port 22
	User root
	AddKeysToAgent yes
	IdentityFile ~/.ssh/id_custom

Host custom.ftp.domen
	HostName custom.ftp.domen
	User user
	AddKeysToAgent yes
	IdentityFile ~/.ssh/id_custom
